{
  "name": "swaggest/php-code-builder",
  "type": "library",
  "description": "Swaggest JSON-schema enabled PHP code builder",
  "keywords": ["library"],
  "homepage": "https://github.com/swaggest/php-code-builder",
  "license": "MIT",
  "authors": [
    {
      "name": "Viacheslav Poturaev",
      "email": "vearutop@gmail.com",
      "homepage": "https://github.com/vearutop"
    }
  ],
  "repositories": [
      {
          "type": "vcs",
          "url": "https://github.com/jhammer/php-json-schema"
      }
  ],
  "require": {
    "ext-json": "*",
    "swaggest/json-schema": "dev-form-builder-1",
    "swaggest/code-builder": "^0.3.4",
    "php": ">=5.6.0"
  },
  "require-dev": {
    "phpunit/phpunit": "4.8.36"
  },
  "autoload": {
    "psr-4": {
      "Swaggest\\PhpCodeBuilder\\": "src/"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "Swaggest\\PhpCodeBuilder\\Tests\\": "tests/src/"
    }
  },
  "config": {
    "platform": {
      "php": "5.6.10"
    }
  },
  "scripts": {
    "post-install-cmd": [
      "patch -N -s -p0 < ./tests/phpunit.patch"
    ],
    "post-update-cmd": [
      "patch -N -s -p0 < ./tests/phpunit.patch"
    ]
  }
}
